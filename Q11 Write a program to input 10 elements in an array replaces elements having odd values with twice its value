/*
Q11 Write a program to input 10 elements in an array replaces elements having odd values with twice its value 
*/

void main()
{cout<<"Enter no of array elemenets";
cin>>n;
int a[n];
for(int i=0;i<n;i++)
{
cin>>a[i];
}
for(int i=0;i<n;i++)
{
if (a[i]%2!=0)
{a[i]=a[i]*2;
}
}
for(int i=0;i<n;i++)
{
cout<<a[i]<<" ";
}


}
